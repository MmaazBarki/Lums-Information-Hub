{
  "dependencies": {
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "nodemailer": "^6.10.1"
  },

  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "scripts": {
    "start:frontend": "cd frontend ; npm run dev",
    "start:backend": "cd backend ; npm run dev",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "update": "concurrently \"cd frontend ; npm i\" \"cd backend ; npm i\""
  }
}
